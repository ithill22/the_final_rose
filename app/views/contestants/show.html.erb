<h1><%= @contestant.name %></h1>
<p>Season <%= @contestant.bachelorette.season_number %> - <%= @contestant.bachelorette.season_description %></p>
<h2>Outings:</h2>
<ul id="outings-list">
  <% @contestant.outings.each do |outing| %>
    <li id="outing-<%= outing.id %>">
      <p><%= link_to outing.name, outing_path(outing) %></p>
    </li>
  <% end %>
</ul>